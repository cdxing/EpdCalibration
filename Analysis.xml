<?xml version="1.0" encoding="utf-8" ?>

 <job minFilesPerProcess="5" maxFilesPerProcess="100" filesPerHour="10" simulateSubmission="false" fileListSyntax="xrootd">

 <stdout discard="true" />

 <command>
   setenv NODEBUG yes
   <!--starver SL21c
   cat $FILELIST | grep -v "root://" > mynewile.list
   setenv FILELIST mynewile.list-->
   root4star -l -q -b RunAnalysis.C\(\"$FILELIST\",\"$JOBID\"\) &gt;&amp; ${JOBID}.log
 </command>

 <SandBox installer="ZIP">
   <Package>
     <File>file:./Analysis.xml </File>
     <File>file:/star/u/dchen/service/14p5GeV_2019/EpdCalibration/RunAnalysis.C </File>
     <File>file:/star/u/dchen/service/14p5GeV_2019/EpdCalibration/StRoot </File>
   </Package>
  </SandBox>

  <input URL="catalog:star.bnl.gov?eroduction=P21ic,filetype=daq_reco_picoDst,trgsetupname=production_14p5GeV_2019,filename~st_physics,sanity=1,storage!=hpss" nFiles="all"/>
  <stdout URL="file:/star/data01/pwg/dchen/Ana/14p5GeV/epd_ep/EpdEp/out/$JOBID.out"/>
